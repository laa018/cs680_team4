<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.bytecode.use_reflection_optimizer">false</property>
        <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/group4_cvsanaly</property>
        <property name="hibernate.connection.username">operator</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
        <property name="hibernate.search.autoregister_listeners">false</property>
        <property name="hibernate.current_session_context_class">thread</property>
        <mapping resource="group4_cvsanaly/FileTypes.hbm.xml" />
        <mapping resource="group4_cvsanaly/Tags.hbm.xml" />
        <mapping resource="group4_cvsanaly/ActionsFileNames.hbm.xml" />
        <mapping resource="group4_cvsanaly/Files.hbm.xml" />
        <mapping resource="group4_cvsanaly/FileLinks.hbm.xml" />
        <mapping resource="group4_cvsanaly/Scmlog.hbm.xml" />
        <mapping resource="group4_cvsanaly/Actions.hbm.xml" />
        <mapping resource="group4_cvsanaly/Branches.hbm.xml" />
        <mapping resource="group4_cvsanaly/ActionFiles.hbm.xml" />
        <mapping resource="group4_cvsanaly/CommitsLines.hbm.xml" />
        <mapping resource="group4_cvsanaly/TagRevisions.hbm.xml" />
        <mapping resource="group4_cvsanaly/Metrics.hbm.xml" />
        <mapping resource="group4_cvsanaly/People.hbm.xml" />
        <mapping resource="group4_cvsanaly/FileCopies.hbm.xml" />
        <mapping resource="group4_cvsanaly/Repositories.hbm.xml" />
    </session-factory>
</hibernate-configuration>
